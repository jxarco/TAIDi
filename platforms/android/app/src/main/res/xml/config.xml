<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.taidi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Screenshot">
        <param name="android-package" value="com.darktalker.cordova.screenshot.Screenshot" />
    </feature>
    <name>TAIDi</name>
    <description>
        An hybrid application to hold your tasks and your shopping list cooperatively.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        G5 ASM Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon density="mdpi" src="res/icon/android/icon-72-mdpi.png" />
    <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    <icon density="xxhdpi" src="res/icon/android/icon-144-xxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>

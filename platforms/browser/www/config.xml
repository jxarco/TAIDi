<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.taidi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TAIDi</name>
    <description>
        An hybrid application to hold your tasks and your shopping list cooperatively.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        G5 ASM Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
